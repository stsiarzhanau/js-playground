{
  "name": "js-playground",
  "version": "0.0.1",
  "description": "transpiler, test environment, linter",
  "author": "Aliaksandr Stsiarzhanau <sterzhanoff@gmail.com>",
  "license": "MIT",
  "scripts": {
    "test": "mocha-webpack --watch",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "precommit": "lint-staged",
    "postcommit": "git update-index -g"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "babel-polyfill": "6.26.0"
  },
  "devDependencies": {
    "babel-core": "6.26.3",
    "babel-eslint": "8.2.5",
    "babel-loader": "7.1.5",
    "babel-preset-env": "1.7.0",
    "babel-preset-stage-0": "6.24.1",
    "chai": "4.1.2",
    "chai-as-promised": "7.1.1",
    "eslint": "4.19.1",
    "eslint-config-airbnb-base": "13.0.0",
    "eslint-import-resolver-webpack": "0.10.1",
    "eslint-plugin-import": "2.13.0",
    "husky": "0.14.3",
    "jsdom": "11.11.0",
    "lint-staged": "7.2.0",
    "mocha": "5.2.0",
    "mocha-webpack": "2.0.0-beta.0",
    "sinon": "6.1.2",
    "sinon-chai": "3.2.0",
    "webpack": "4.15.1",
    "webpack-node-externals": "1.7.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/stsiarzhanau/js-playground.git"
  }
}
